<h1>Editing Student #<%= @student.id %></h1>

<%= form_tag(student_url(@student), method: 'patch') do %>
  <div>
    <%= label_tag :first_name %><br />
    <%= text_field_tag :first_name, @student.first_name %>
  </div>

  <div>
    <%= label_tag :last_name %><br />
    <%= text_field_tag :last_name, @student.last_name %>
  </div>

  <div>
    <%= submit_tag "Update Student" %>
  </div>
<% end %>
